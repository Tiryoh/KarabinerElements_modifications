{
  "title": "Gyazo settings",
  "rules": [
    {
      "description": "⌘+⇧ +4でGyazo起動",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "4" }
            ],
            "modifiers": {
              "mandatory": ["command", "shift"],
              "optional": [ "any" ]
            }
          },
          "to": [
            { "shell_command": "open -a 'Gyazo.app'" }
          ]
        }
      ]
    },
    {
      "description": "⌘+⇧ +5でGyazo起動",
